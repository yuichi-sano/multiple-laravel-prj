                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="packages\domain\model\prefecture\Prefecture" table="sample.prefectures">
        <embedded
                name="prefectureCode"
                class="packages\domain\model\prefecture\PrefectureCode"
                use-column-prefix="false"
        />
        <embedded
                name="prefectureName"
                class="packages\domain\model\prefecture\PrefectureName"
                use-column-prefix="false"
        />
    </entity>
</doctrine-mapping>
